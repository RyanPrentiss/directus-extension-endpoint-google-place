{
  "name": "@rockdog/directus-extension-endpoint-google-place",
  "description": "A Directus extension to fetch Google Places data",
  "icon": "extension",
  "version": "0.0.0-development",
  "author": {
    "name": "Rock Dog Interactive",
    "email": "info@rockdog.dev"
  },
  "repository": {
